{
  "name": "@webapp/ui",
  "version": "0.1.0",
  "basedir": "../..",
  "license": "MIT",
  "scripts": {
    "clean": "npx shx rm -rf build",
    "prebuild": "npm run clean",
    "build": "npx cross-env NODE_ENV=production react-app-rewired build",
    "start": "npx cross-env NODE_ENV=development react-app-rewired start",
    "test": "npx cross-env CI=true react-app-rewired test --setupTestFrameworkScriptFile=./etc/setup-enzyme.js"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "dependencies": {
    "antd": "^3.7.0",
    "autoprefixer": "^9.0.0",
    "browserslist": "^4.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react": "^16.4.1",
    "react-app-rewire-less": "^2.1.2",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-scripts": "^1.1.4"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.8.0"
  }
}
